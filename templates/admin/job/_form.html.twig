{{ form_start(form) }}
<div class="row">
    <div class="col-md-4"> {{ form_row(form.title) }}</div>
    <div class="col-md-4"> {{ form_row(form.experience) }}</div>
    <div class="col-md-4"> {{ form_row(form.salary) }}</div>
</div>
<div class="row">
    <div class="col-md-5"> {{ form_row(form.address) }}</div>
    <div class="col-md-2"> {{ form_row(form.city) }}</div>
    <div class="col-md-1"> {{ form_row(form.postal_code) }}</div>
    <div class="col-md-2"> {{ form_row(form.is_available) }}</div>
</div>

{{ form_widget(form) }}
<button class="btn btn-primary"> {{ button|default('Enregistrer') }} </button>
{{ form_end(form) }}